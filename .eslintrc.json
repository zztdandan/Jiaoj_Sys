{
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "jquery": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 2015
  },
  "globals": {
    "$": false,
    "jquery": false
  },
  "rules": {
    "no-console": "off",
    "indent": "off",
    "linebreak-style": ["off", "windows"],
    "quotes": ["error", "single"],
    "semi": ["error", "always"],
    "no-unused-vars": "off"
  }
}
